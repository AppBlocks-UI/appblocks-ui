{
  "name": "@yogo-ui/theme",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./src": {
      "default": "./src/index.ts"
    },
    "./anatomy": {
      "require": "./dist/anatomy/index.js",
      "types": "./dist/anatomy/index.d.ts",
      "import": "./dist/anatomy/index/.mjs"
    },
    "./anatomy/src": {
      "default": "./src/anatomy/index.ts"
    }
  },
  "typesVersions": {
    "*": {
      "anatomy": [
        "./dist/anatomy/index.d.ts"
      ]
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf --no-glob ./dist",
    "build": "tsup src/index.ts src/anatomy/index.ts --config tsup.config.ts",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --config ../../.eslintrc.js",
    "lint:staged": "lint-staged --allow-empty --config ../../lint-staged.config.js",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "Appblocks Inc. <appblocks.xyz@gmail.com>",
  "homepage": "https://appblocks.xyz/",
  "repository": {
    "type": "git",
    "url": "https://github.com/AppBlocks-UI/appblocks-ui",
    "directory": "packages/yogo-ui-react"
  },
  "keywords": [
      "appblocks-ui",
      "react",
      "ui",
      "premium",
      "chakra-ui",
      "design-system",
      "react-components",
      "uikit",
      "accessible",
      "components",
      "emotion",
      "library"
  ],
  "dependencies": {
    "@chakra-ui/anatomy": "^2.1.2",
    "@chakra-ui/styled-system": "^2.9.1",
    "@chakra-ui/theme-tools": "^2.0.18"
  },
  "peerDependencies": {
    "@chakra-ui/react": ">=2.4.6",
    "@chakra-ui/system": ">=2.3.8",
    "react": ">=18.0.0"
  }
}
